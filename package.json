{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "test": "craco test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "dev": "STORYBOOK_GRAPHQL_HOST=$DOCKER_MACHINE_NAME:4000 start-storybook --debug-webpack -p 9001 -c .storybook",
    "build": "npm run build:ssr && npm run build:client && node build/ssr.js && rm build/ssr.js",
    "build:ssr": "webpack --config webpack.ssr.js",
    "build:client": "webpack --config webpack.client.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.13",
    "cross-fetch": "^3.1.4",
    "graphql": "^16.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1",
    "emotion": "^10.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/cli": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-ant-design": "^0.0.2",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "babel-plugin-import": "^1.13.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-graphql-tag": "^3.2.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "@babel/cli": "^7.13.0",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/helper-plugin-utils": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.9",
    "@babel/runtime-corejs3": "^7.13.9",
    "@emotion/react": "^11.4.0",
    "less-loader": "^10.2.0",
    "css-loader": "^5.2.7",
    "emotion-reset": "^3.0.0",
    "emotion-server": "^10.0.27",
    "file-loader": "^6.2.0",
    "postcss-loader": "^4.3.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
